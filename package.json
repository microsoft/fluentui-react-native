    "es5-ext": "0.10.53",
    "jpeg-js": "^0.4.4",
    "moment": "^2.29.4",
    "shell-quote": "^1.7.3",
    "@types/react": "^18.2.0",
    "@appium/types": "0.9.1",
    "micromatch": "^4.0.0",
    "react-native-svg": "^13.7.0",
    "xml2js": "^0.5.0",
    "yaml": "^2.2.2"
  },
  "rnx-kit": {
    "kitType": "library",
    "alignDeps": {
      "presets": [
        "microsoft/react-native"
      ],
      "requirements": [
        "react-native@0.71"
      ],
      "capabilities": [
        "babel-preset-react-native",
        "core",
        "react-dom"
      ]
    }
  }
