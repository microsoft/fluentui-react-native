    "test": "lage test",
    "test-links": "markdown-link-check"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/preset-env": "^7.8.0",
    "@babel/preset-react": "^7.8.0",
    "@babel/preset-typescript": "^7.8.0",
    "@rnx-kit/dep-check": "^1.10.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "beachball": "^2.20.0",
    "lage": "^1.5.0",
