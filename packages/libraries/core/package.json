{
  "name": "@fluentui/react-native",
  "version": "0.27.4",
  "description": "A react-native component library that implements the Fluent Design System.",
  "repository": {
    "type": "git",
    "url": "git@github.com:microsoft/fluentui-react-native.git",
    "directory": "packages/libraries/core"
  },
  "main": "src/index.ts",
  "module": "src/index.ts",
  "onPublish": {
    "main": "lib-commonjs/index.js",
    "module": "lib/index.js"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "fluentui-scripts build",
    "bundle": "fluentui-scripts bundle",
    "clean": "fluentui-scripts clean",
    "code-style": "fluentui-scripts code-style",
    "depcheck": "fluentui-scripts depcheck",
    "just": "fluentui-scripts",
    "lint": "fluentui-scripts eslint",
    "start:tester": "react-native start --projectRoot ./src",
    "start-test": "fluentui-scripts jest-watch",
    "test": "fluentui-scripts jest",
    "update-snapshots": "fluentui-scripts jest -u",
    "prettier": "fluentui-scripts prettier",
    "prettier-fix": "fluentui-scripts prettier --fix true"
  },
  "dependencies": {
    "@fluentui-react-native/button": ">=0.21.3 <1.0.0",
    "@fluentui-react-native/callout": ">=0.19.4 <1.0.0",
    "@fluentui-react-native/checkbox": ">=0.14.3 <1.0.0",
    "@fluentui-react-native/contextual-menu": "0.13.3",
    "@fluentui-react-native/focus-trap-zone": ">=0.8.3 <1.0.0",
    "@fluentui-react-native/focus-zone": ">=0.8.3 <1.0.0",
    "@fluentui-react-native/interactive-hooks": ">=0.11.3 <1.0.0",
    "@fluentui-react-native/link": ">=0.15.3 <1.0.0",
    "@fluentui-react-native/menu-button": ">=0.7.4 <1.0.0",
    "@fluentui-react-native/persona": ">=0.12.3 <1.0.0",
    "@fluentui-react-native/persona-coin": ">=0.10.3 <1.0.0",
    "@fluentui-react-native/pressable": ">=0.8.3 <1.0.0",
    "@fluentui-react-native/radio-group": ">=0.14.3 <1.0.0",
    "@fluentui-react-native/separator": ">=0.11.3 <1.0.0",
    "@fluentui-react-native/tabs": ">=0.6.3 <1.0.0",
    "@fluentui-react-native/text": ">=0.11.2 <1.0.0"
  },
  "devDependencies": {
    "@fluentui-react-native/eslint-config-rules": "^0.1.1",
    "@types/react-native": "^0.64.0",
    "@uifabricshared/build-native": "^0.1.1",
    "react": "17.0.1",
    "react-native": "^0.64.3"
  },
  "peerDependencies": {
    "react": ">=17.0.1",
    "react-native": ">=0.64.3"
  },
  "author": "",
  "license": "MIT"
}
