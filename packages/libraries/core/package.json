{
  "name": "@fluentui/react-native",
  "version": "0.15.0",
  "description": "A react-native companion to fabric web that only depends on stock controls and theming",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "onPublish": {
    "main": "lib-commonjs/index.js",
    "module": "lib/index.js"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "fluentui-scripts build",
    "just": "fluentui-scripts",
    "clean": "fluentui-scripts clean",
    "code-style": "fluentui-scripts code-style",
    "lint": "fluentui-scripts eslint",
    "bundle": "fluentui-scripts metro",
    "start:tester": "react-native start --projectRoot ./src",
    "start-test": "fluentui-scripts jest-watch",
    "update-snapshots": "fluentui-scripts jest -u",
    "verify-api": "fluentui-scripts verify-api-extractor",
    "update-api": "fluentui-scripts update-api-extractor"
  },
  "dependencies": {
    "@uifabricshared/foundation-composable": "0.6.0",
    "@uifabricshared/foundation-compose": "1.6.0",
    "@uifabricshared/foundation-settings": "0.5.0",
    "@uifabricshared/foundation-tokens": "0.5.0",
    "@uifabricshared/immutable-merge": "0.5.0",
    "@uifabricshared/theming-ramp": "0.8.0",
    "@uifabricshared/theming-react-native": "0.6.0",
    "@fluentui-react-native/adapters": "0.2.0",
    "@fluentui-react-native/interactive-hooks": "0.3.0",
    "@fluentui-react-native/button": "0.4.0",
    "@fluentui-react-native/callout": "0.4.0",
    "@fluentui-react-native/checkbox": "0.3.0",
    "@fluentui-react-native/focus-trap-zone": "0.3.0",
    "@fluentui-react-native/link": "0.4.0",
    "@fluentui-react-native/persona": "0.4.0",
    "@fluentui-react-native/persona-coin": "0.3.0",
    "@fluentui-react-native/pressable": "0.3.0",
    "@fluentui-react-native/radio-group": "0.3.0",
    "@fluentui-react-native/separator": "0.3.0",
    "@fluentui-react-native/text": "0.4.0",
    "@fluentui-react-native/tokens": "0.3.0"
  },
  "devDependencies": {
    "@office-iss/react-native-win32": "0.27.3",
    "@types/prop-types": "15.5.1",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.5",
    "@types/react-native": "^0.60.0",
    "@uifabricshared/build-native": "^0.1.1",
    "@uifabricshared/eslint-config-rules": "^0.1.1",
    "react": "16.8.6",
    "react-native": "^0.60.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-native": "^0.59.0",
    "@office-iss/react-native-win32": "0.27.3"
  },
  "metroBundles": {
    "name": "uifabric",
    "targets": [
      "win32"
    ],
    "entry": "./src/index.ts",
    "outputPath": "./dist",
    "bundleName": "fluentui-react-native.bundle"
  },
  "author": "",
  "license": "MIT"
}
