{
  "name": "@fluentui/react-native",
  "version": "0.32.34",
  "description": "A react-native component library that implements the Fluent Design System.",
  "repository": {
    "type": "git",
    "url": "git@github.com:microsoft/fluentui-react-native.git",
    "directory": "packages/libraries/core"
  },
  "main": "src/index.ts",
  "module": "src/index.ts",
  "onPublish": {
    "main": "lib-commonjs/index.js",
    "module": "lib/index.js"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "fluentui-scripts build",
    "clean": "fluentui-scripts clean",
    "depcheck": "fluentui-scripts depcheck",
    "just": "fluentui-scripts",
    "lint": "fluentui-scripts eslint",
    "start:tester": "react-native start --projectRoot ./src",
    "start-test": "fluentui-scripts jest-watch",
    "test": "fluentui-scripts jest",
    "update-snapshots": "fluentui-scripts jest -u",
    "prettier": "fluentui-scripts prettier",
    "prettier-fix": "fluentui-scripts prettier --fix true"
  },
  "dependencies": {
    "@fluentui-react-native/button": "0.25.9",
    "@fluentui-react-native/callout": "0.21.21",
    "@fluentui-react-native/checkbox": "0.16.24",
    "@fluentui-react-native/contextual-menu": "0.18.23",
    "@fluentui-react-native/focus-trap-zone": "0.9.20",
    "@fluentui-react-native/focus-zone": "0.10.21",
    "@fluentui-react-native/interactive-hooks": "0.18.13",
    "@fluentui-react-native/link": "0.16.19",
    "@fluentui-react-native/menu-button": "0.8.43",
    "@fluentui-react-native/persona": "0.13.11",
    "@fluentui-react-native/persona-coin": "0.11.11",
    "@fluentui-react-native/pressable": "0.9.19",
    "@fluentui-react-native/radio-group": "0.15.23",
    "@fluentui-react-native/separator": "0.12.10",
    "@fluentui-react-native/tabs": "0.7.22",
    "@fluentui-react-native/text": "0.15.6"
  },
  "devDependencies": {
    "@fluentui-react-native/eslint-config-rules": "^0.1.1",
    "@fluentui-react-native/scripts": "^0.1.1",
    "@types/react-native": "^0.68.0",
    "react": "17.0.2",
    "react-native": "^0.68.0"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-native": "^0.68.0"
  },
  "author": "",
  "license": "MIT",
  "rnx-kit": {
    "reactNativeVersion": "^0.68",
    "reactNativeDevVersion": "^0.68",
    "kitType": "library",
    "capabilities": [
      "core",
      "core-android",
      "core-ios",
      "react"
    ]
  }
}
