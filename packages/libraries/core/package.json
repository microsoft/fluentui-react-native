{
  "name": "@fluentui/react-native",
  "version": "0.24.22",
  "description": "A react-native component library that implements the Fluent Design System.",
  "repository": {
    "type": "git",
    "url": "git@github.com:microsoft/fluentui-react-native.git",
    "directory": "packages/libraries/core"
  },
  "main": "src/index.ts",
  "module": "src/index.ts",
  "onPublish": {
    "main": "lib-commonjs/index.js",
    "module": "lib/index.js"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "fluentui-scripts build",
    "bundle": "fluentui-scripts bundle",
    "clean": "fluentui-scripts clean",
    "code-style": "fluentui-scripts code-style",
    "depcheck": "fluentui-scripts depcheck",
    "just": "fluentui-scripts",
    "lint": "fluentui-scripts eslint",
    "start:tester": "react-native start --projectRoot ./src",
    "start-test": "fluentui-scripts jest-watch",
    "test": "fluentui-scripts jest",
    "update-api": "fluentui-scripts update-api-extractor",
    "update-snapshots": "fluentui-scripts jest -u",
    "verify-api": "fluentui-scripts verify-api-extractor",
    "prettier": "fluentui-scripts prettier",
    "prettier-fix": "fluentui-scripts prettier --fix true"
  },
  "dependencies": {
    "@fluentui-react-native/button": ">=0.17.21 <1.0.0",
    "@fluentui-react-native/callout": ">=0.16.11 <1.0.0",
    "@fluentui-react-native/checkbox": ">=0.11.22 <1.0.0",
    "@fluentui-react-native/contextual-menu": "0.9.17",
    "@fluentui-react-native/focus-trap-zone": ">=0.7.23 <1.0.0",
    "@fluentui-react-native/focus-zone": ">=0.6.12 <1.0.0",
    "@fluentui-react-native/interactive-hooks": ">=0.10.22 <1.0.0",
    "@fluentui-react-native/link": ">=0.12.24 <1.0.0",
    "@fluentui-react-native/menu-button": ">=0.3.0 <1.0.0",
    "@fluentui-react-native/persona": ">=0.10.15 <1.0.0",
    "@fluentui-react-native/persona-coin": ">=0.8.14 <1.0.0",
    "@fluentui-react-native/pressable": ">=0.6.23 <1.0.0",
    "@fluentui-react-native/radio-group": ">=0.10.23 <1.0.0",
    "@fluentui-react-native/separator": ">=0.10.17 <1.0.0",
    "@fluentui-react-native/text": ">=0.9.16 <1.0.0"
  },
  "devDependencies": {
    "@types/react-native": "^0.63.0",
    "@uifabricshared/build-native": "^0.1.1",
    "@uifabricshared/eslint-config-rules": "^0.1.1",
    "react": "16.13.1",
    "react-native": "^0.63.4"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-native": ">=0.63.4"
  },
  "author": "",
  "license": "MIT"
}
