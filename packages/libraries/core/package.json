{
  "name": "@fluentui/react-native",
  "version": "0.21.7",
  "description": "A react-native component library that implements the Fluent Design System.",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "onPublish": {
    "main": "lib-commonjs/index.js",
    "module": "lib/index.js"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "fluentui-scripts build",
    "bundle": "fluentui-scripts bundle",
    "clean": "fluentui-scripts clean",
    "code-style": "fluentui-scripts code-style",
    "depcheck": "fluentui-scripts depcheck",
    "just": "fluentui-scripts",
    "lint": "fluentui-scripts eslint",
    "start:tester": "react-native start --projectRoot ./src",
    "start-test": "fluentui-scripts jest-watch",
    "test": "fluentui-scripts jest",
    "update-api": "fluentui-scripts update-api-extractor",
    "update-snapshots": "fluentui-scripts jest -u",
    "verify-api": "fluentui-scripts verify-api-extractor"
  },
  "dependencies": {
    "@fluentui-react-native/interactive-hooks": ">=0.7.0 <1.0.0",
    "@fluentui-react-native/button": ">=0.11.1 <1.0.0",
    "@fluentui-react-native/callout": ">=0.12.1 <1.0.0",
    "@fluentui-react-native/checkbox": ">=0.8.2 <1.0.0",
    "@fluentui-react-native/contextual-menu": "0.5.1",
    "@fluentui-react-native/focus-trap-zone": ">=0.5.0 <1.0.0",
    "@fluentui-react-native/focus-zone": ">=0.3.0 <1.0.0",
    "@fluentui-react-native/link": ">=0.10.1 <1.0.0",
    "@fluentui-react-native/persona": ">=0.7.1 <1.0.0",
    "@fluentui-react-native/persona-coin": ">=0.5.1 <1.0.0",
    "@fluentui-react-native/pressable": ">=0.4.0 <1.0.0",
    "@fluentui-react-native/radio-group": ">=0.8.1 <1.0.0",
    "@fluentui-react-native/separator": ">=0.7.2 <1.0.0",
    "@fluentui-react-native/shimmer": ">=0.3.3 <1.0.0",
    "@fluentui-react-native/spinner": ">=0.1.0 <1.0.0",
    "@fluentui-react-native/text": ">=0.7.1 <1.0.0"
  },
  "devDependencies": {
    "@types/react-native": "^0.62.0",
    "@uifabricshared/build-native": "^0.1.1",
    "@uifabricshared/eslint-config-rules": "^0.1.1",
    "react": "16.11.0",
    "react-native": "0.62.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-native": ">=0.60.0"
