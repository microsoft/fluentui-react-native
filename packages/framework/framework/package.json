{
  "name": "@fluentui-react-native/framework",
  "version": "0.7.28",
  "description": "Component framework used by fluentui react native controls",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "onPublish": {
    "main": "lib-commonjs/index.js",
    "module": "lib/index.js"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "fluentui-scripts build",
    "clean": "fluentui-scripts clean",
    "depcheck": "fluentui-scripts depcheck",
    "just": "fluentui-scripts",
    "lint": "fluentui-scripts eslint",
    "start:tester": "react-native start --projectRoot ./src",
    "start-test": "fluentui-scripts jest-watch",
    "test": "fluentui-scripts jest",
    "update-snapshots": "fluentui-scripts jest -u",
    "prettier": "fluentui-scripts prettier",
    "prettier-fix": "fluentui-scripts prettier --fix true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/fluentui-react-native.git",
    "directory": "packages/framework/framework"
  },
  "dependencies": {
    "@fluentui-react-native/composition": ">=0.7.7 <1.0.0",
    "@fluentui-react-native/default-theme": ">=0.11.0 <1.0.0",
    "@fluentui-react-native/immutable-merge": "^1.1.7",
    "@fluentui-react-native/merge-props": ">=0.4.3 <1.0.0",
    "@fluentui-react-native/theme-types": ">=0.15.0 <1.0.0",
    "@fluentui-react-native/use-styling": ">=0.8.3 <1.0.0",
    "@fluentui-react-native/use-slot": ">=0.2.6 <1.0.0",
    "@fluentui-react-native/use-slots": ">=0.6.6 <1.0.0",
    "@fluentui-react-native/use-tokens": ">=0.2.3 <1.0.0",
    "@fluentui-react-native/memo-cache": "^1.1.7",
    "@fluentui-react-native/tokens": ">=0.12.2 <1.0.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@fluentui-react-native/eslint-config-rules": "^0.1.1",
    "@types/react": "^17.0.2",
    "@types/react-native": "^0.64.0",
    "@fluentui-react-native/scripts": "^0.1.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "^0.64.3"
  },
  "peerDependencies": {
    "react": ">=17.0.1",
    "react-native": ">=0.64.3"
  },
  "author": "",
  "license": "MIT"
}
