{
  "name": "@fluentui-react-native/framework",
  "version": "0.7.3",
  "description": "Component framework used by fluentui react native controls",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "onPublish": {
    "main": "lib-commonjs/index.js",
    "module": "lib/index.js"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "fluentui-scripts build",
    "bundle": "fluentui-scripts webpack",
    "clean": "fluentui-scripts clean",
    "code-style": "fluentui-scripts code-style",
    "depcheck": "fluentui-scripts depcheck",
    "just": "fluentui-scripts",
    "lint": "fluentui-scripts eslint",
    "start:tester": "react-native start --projectRoot ./src",
    "start-test": "fluentui-scripts jest-watch",
    "test": "fluentui-scripts jest",
    "update-snapshots": "fluentui-scripts jest -u",
    "prettier": "fluentui-scripts prettier",
    "prettier-fix": "fluentui-scripts prettier --fix true"
  },
  "dependencies": {
    "@fluentui-react-native/composition": ">=0.7.1 <1.0.0",
    "@fluentui-react-native/default-theme": ">=0.9.2 <1.0.0",
    "@fluentui-react-native/immutable-merge": "^1.1.4",
    "@fluentui-react-native/merge-props": ">=0.4.0 <1.0.0",
    "@fluentui-react-native/theme-types": ">=0.13.0 <1.0.0",
    "@fluentui-react-native/use-styling": ">=0.8.0 <1.0.0",
    "@fluentui-react-native/use-slot": ">=0.2.1 <1.0.0",
    "@fluentui-react-native/use-slots": ">=0.6.1 <1.0.0",
    "@fluentui-react-native/use-tokens": ">=0.2.0 <1.0.0",
    "@fluentui-react-native/memo-cache": "^1.1.5",
    "@fluentui-react-native/tokens": ">=0.11.1 <1.0.0",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@fluentui-react-native/eslint-config-rules": "^0.1.1",
    "@types/react": "^17.0.2",
    "@types/react-native": "^0.64.0",
    "@uifabricshared/build-native": "^0.1.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "^0.64.3",
    "react-native-web": "0.17.5"
  },
  "peerDependencies": {
    "react": ">=17.0.1",
    "react-native": ">=0.64.3"
  },
  "author": "",
  "license": "MIT"
}
