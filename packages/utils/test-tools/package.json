    "@fluentui-react-native/jest-config": "workspace:*",
    "@fluentui-react-native/scripts": "workspace:*",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.2.0",
    "@types/react-test-renderer": "^18.2.0",
    "react": "18.2.0",
    "react-native": "^0.74.0",
    "react-test-renderer": "18.2.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-native": "^0.73.0 || ^0.74.0"
