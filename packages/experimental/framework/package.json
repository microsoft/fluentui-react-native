{
  "name": "@fluentui-react-native/framework",
  "version": "0.5.8",
  "description": "Component framework used by fluentui react native controls",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "onPublish": {
    "main": "lib-commonjs/index.js",
    "module": "lib/index.js"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "fluentui-scripts build",
    "bundle": "fluentui-scripts webpack",
    "clean": "fluentui-scripts clean",
    "code-style": "fluentui-scripts code-style",
    "depcheck": "fluentui-scripts depcheck",
    "just": "fluentui-scripts",
    "lint": "fluentui-scripts eslint",
    "start:tester": "react-native start --projectRoot ./src",
    "start-test": "fluentui-scripts jest-watch",
    "test": "fluentui-scripts jest",
    "update-snapshots": "fluentui-scripts jest -u",
    "verify-api": "fluentui-scripts verify-api-extractor",
    "update-api": "fluentui-scripts update-api-extractor",
    "prettier": "fluentui-scripts prettier",
    "prettier-fix": "fluentui-scripts prettier --fix true"
  },
  "dependencies": {
    "@fluentui-react-native/composition": ">=0.6.1 <1.0.0",
    "@fluentui-react-native/default-theme": ">=0.6.5 <1.0.0",
    "@fluentui-react-native/immutable-merge": "^1.1.1",
    "@fluentui-react-native/merge-props": ">=0.3.1 <1.0.0",
    "@fluentui-react-native/theme-types": ">=0.8.0 <1.0.0",
    "@fluentui-react-native/use-styling": ">=0.6.1 <1.0.0",
    "@fluentui-react-native/use-slots": ">=0.5.2 <1.0.0",
    "@fluentui-react-native/memo-cache": "^1.1.1",
    "@fluentui-react-native/tokens": ">=0.9.4 <1.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-native": "^0.63.0",
    "@uifabricshared/build-native": "^0.1.1",
    "@uifabricshared/eslint-config-rules": "^0.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "^0.63.4",
    "react-native-web": "0.12.3"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-native": ">=0.63.4"
  },
  "author": "",
  "license": "MIT"
}
