{
  "$schema": "https://raw.githubusercontent.com/microsoft/fluentui-token-pipeline/main/docs/token.schema.json",
  "Meta": {
    "FluentUITokensVersion": 0
  },
  "Set": {
    "NeutralForeground2": {
      "Fill": {
        "Color": {
          "BrandHover": { "aliasOf": "Global.Color.Brand.Tint.40" },
          "BrandPressed": { "aliasOf": "Global.Color.Brand.Tint.10" },
          "BrandSelected": { "aliasOf": "Global.Color.Brand.Tint.40" }
        }
      }
    },
    "NeutralForeground3": {
      "Fill": {
        "Color": {
          "BrandHover": { "aliasOf": "Global.Color.Brand.Tint.40" },
          "BrandPressed": { "aliasOf": "Global.Color.Brand.Tint.10" },
          "BrandSelected": { "aliasOf": "Global.Color.Brand.Tint.40" }
        }
      }
    },

    "BrandForegroundLink": {
      "Fill": {
        "Color": {
          "Rest": { "aliasOf": "Global.Color.Brand.Tint.30" },
          "Hover": { "aliasOf": "Global.Color.Brand.Tint.40" },
          "Pressed": { "aliasOf": "Global.Color.Brand.Tint.10" },
          "Selected": { "aliasOf": "Global.Color.Brand.Tint.40" }
        }
      }
    },
    "CompoundBrandForeground1": {
      "Fill": {
        "Color": {
          "Rest": { "aliasOf": "Global.Color.Brand.Tint.30" },
          "Hover": { "aliasOf": "Global.Color.Brand.Tint.40" },
          "Pressed": { "aliasOf": "Global.Color.Brand.Tint.10" }
        }
      }
    },
    "BrandForeground1": {
      "Fill": {
        "Color": {
          "Rest": { "aliasOf": "Global.Color.Brand.Tint.30" }
        }
      }
    },
    "BrandForeground2": {
      "Fill": {
        "Color": {
          "Rest": { "aliasOf": "Global.Color.Brand.Tint.40" }
        }
      }
    },

    "NeutralForegroundInverted": {
      "Fill": {
        "Color": {
          "Rest": { "aliasOf": "Global.Color.Grey.0" }
        }
      }
    },
    "NeutralForegroundInvertedLink": {
      "Fill": {
        "Color": {
          "Rest": { "aliasOf": "Global.Color.Grey.16" },
          "Hover": { "aliasOf": "Global.Color.Grey.16" },
          "Pressed": { "aliasOf": "Global.Color.Grey.16" },
          "Selected": { "aliasOf": "Global.Color.Grey.16" }
        }
      }
    },

    "SubtleBackground": {
      "Fill": {
        "Color": {
          "Rest": { "value": "transparent" },
          "Hover": { "aliasOf": "Global.Color.Grey.24" },
          "Pressed": { "aliasOf": "Global.Color.Grey.12" },
          "Selected": { "aliasOf": "Global.Color.Grey.22" }
        }
      }
    },

    "BrandBackground": {
      "Fill": {
        "Color": {
          "Rest": { "aliasOf": "Global.Color.Brand.Primary" },
          "Hover": { "aliasOf": "Global.Color.Brand.Shade.10" },
          "Pressed": { "aliasOf": "Global.Color.Brand.Shade.30" },
          "Selected": { "aliasOf": "Global.Color.Brand.Shade.20" }
        }
      }
    },
    "CompoundBrandBackground1": {
      "Fill": {
        "Color": {
          "Rest": { "aliasOf": "Global.Color.Brand.Primary" },
          "Hover": { "aliasOf": "Global.Color.Brand.Shade.10" },
          "Pressed": { "aliasOf": "Global.Color.Brand.Shade.30" }
        }
      }
    },
    "BrandBackgroundStatic": {
      "Fill": {
        "Color": {
          "Rest": { "aliasOf": "Global.Color.Brand.Primary" }
        }
      }
    },
    "BrandBackground2": {
      "Fill": {
        "Color": {
          "Rest": { "aliasOf": "Global.Color.Brand.Tint.40" }
        }
      }
    },

    "NeutralStrokeAccessible": {
      "Stroke": {
        "Color": {
          "Selected": { "aliasOf": "Global.Color.Brand.Primary" }
        }
      }
    },
    "BrandStroke1": {
      "Stroke": {
        "Color": {
          "Rest": { "aliasOf": "Global.Color.Brand.Primary" }
        }
      }
    },
    "BrandStroke2": {
      "Stroke": {
        "Color": {
          "Rest": { "aliasOf": "Global.Color.Brand.Tint.20" }
        }
      }
    },
    "CompoundBrandStroke1": {
      "Stroke": {
        "Color": {
          "Rest": { "aliasOf": "Global.Color.Brand.Primary" },
          "Hover": { "aliasOf": "Global.Color.Brand.Shade.10" },
          "Pressed": { "aliasOf": "Global.Color.Brand.Shade.30" }
        }
      }
    }
  }
}
