{
  "name": "@fluentui-react-native/tester-macos",
  "version": "0.0.1",
  "private": true,
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "fluentui-scripts build",
    "bundle": "react-native rnx-bundle --dev false",
    "just": "fluentui-scripts",
    "clean": "fluentui-scripts clean",
    "code-style": "fluentui-scripts code-style",
    "depcheck": "fluentui-scripts depcheck",
    "lint": "fluentui-scripts eslint",
    "start": "react-native start",
    "test": "fluentui-scripts jest",
    "macos": "react-native run-macos --scheme FluentTester --project-path src",
    "prettier": "fluentui-scripts prettier",
    "prettier-fix": "fluentui-scripts prettier --fix true",
    "e2etest": "wdio"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/fluentui-react-native.git",
    "directory": "apps/macos"
  },
  "dependencies": {
    "@fluentui-react-native/tester": "^0.50.28",
    "react": "17.0.1",
    "react-native": "^0.64.2",
    "react-native-macos": "^0.64.0",
    "react-native-svg": "^12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/runtime": "^7.8.0",
    "@fluentui-react-native/eslint-config-rules": "^0.1.1",
    "@rnx-kit/cli": "^0.9.57",
    "@rnx-kit/metro-config": "^1.2.26",
    "@fluentui-react-native/scripts": "^0.1.1",
    "@wdio/appium-service": "^6.5.0",
    "@wdio/cli": "^6.5.0",
    "@wdio/jasmine-framework": "^6.5.0",
    "@wdio/local-runner": "^6.5.0",
    "@wdio/spec-reporter": "^6.5.0",
    "@wdio/sync": "^6.5.0",
    "appium-mac2-driver": "0.14.1",
    "flow-bin": "^0.113.0",
    "metro-config": "^0.66.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-test-app": "^0.10.2",
    "react-test-renderer": "17.0.1",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.5.4",
    "webdriverio": "^6.5.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnx-kit": {
    "reactNativeVersion": "0.64",
    "kitType": "app",
    "bundle": {
      "targets": [
        "macos"
      ],
      "entryPath": "index.js",
      "detectCyclicDependencies": {
        "throwOnError": true
      },
      "detectDuplicateDependencies": {
        "throwOnError": false
      },
      "typescriptValidation": false
    },
    "capabilities": [
      "core",
      "core-macos",
      "react",
      "test-app"
    ]
  },
  "depcheck": {
    "ignoreMatches": [
      "react-native-test-app",
      "react-native-svg"
    ]
  }
}
