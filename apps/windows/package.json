{
  "name": "@fluentui-react-native/tester-windows",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "bundle": "react-native rnx-bundle --dev false",
    "depcheck": "fluentui-scripts depcheck",
    "e2etest": "rimraf reports/* && wdio",
    "generate-report": "allure generate allure-results --clean && allure open",
    "lint": "eslint .",
    "report": "allure generate allure-results --clean",
    "start": "react-native start",
    "test": "jest",
    "windows": "react-native run-windows"
  },
  "dependencies": {
    "@fluentui-react-native/tester": "^0.31.1",
    "react": "16.13.1",
    "react-native": "^0.63.4",
    "react-native-windows": "^0.62.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.1.0",
    "@rnx-kit/cli": "^0.5.27",
    "@uifabricshared/build-native": "0.1.1",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-config": "^0.58.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "~16.13.1",
    "rimraf": "~3.0.2",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "3.8.3",
    "@types/jasmine": "3.5.10",
    "allure-commandline": "2.13.0",
    "appium": "1.17.1",
    "webdriverio": "5.22.4",
    "@wdio/allure-reporter": "5.22.4",
    "@wdio/appium-service": "5.18.2",
    "@wdio/cli": "5.23.0",
    "@wdio/jasmine-framework": "5.18.6",
    "@wdio/local-runner": "5.23.0",
    "@wdio/spec-reporter": "5.22.4",
    "@wdio/sync": "5.20.1"
  },
  "workspaces": {
    "nohoist": [
      "@types/jasmine"
    ]
  },
  "jest": {
    "preset": "react-native"
  },
  "rnx-kit": {
    "reactNativeVersion": "0.63",
    "bundle": {
      "targets": [
        "windows"
      ],
      "entryPath": "index.js"
    }
  },
  "depcheck": {
    "ignoreMatches": [
      "react-native-windows"
    ]
  }
}
