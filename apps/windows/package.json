{
  "name": "FluentTester-windows",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "windows": "react-native run-windows",
    "bundle": "fluentui-scripts metro --cli",
    "e2etest": "rimraf reports/* && wdio",
    "run_e2e": "rimraf reports/* && react-native run-windows && wdio"
  },
  "dependencies": {
    "@fluentui-react-native/tester": "^0.0.1",
    "@uifabricshared/theming-react-native": "0.7.31",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-windows": "0.0.0-master.47"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.1.0",
    "@uifabricshared/build-native": "0.1.1",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-config": "^0.58.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "~16.11.0",
    "appium": "1.17.1",
    "@wdio/cli": "^5.22.4",
    "@wdio/jasmine-framework": "^5.18.6",
    "@wdio/local-runner": "^5.22.4",
    "@wdio/sync": "^5.20.1",
    "@wdio/spec-reporter": "^5.22.4",
    "@wdio/appium-service": "^5.0.0",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "3.8.3",
    "@types/jasmine": "3.5.10",
    "webdriverio": "5.22.4"
  },
  "workspaces": {
    "nohoist": [
      "@types/jasmine"
    ]
  },
  "jest": {
    "preset": "react-native"
  },
  "metroBundles": {
    "targets": [
      "windows"
    ],
    "entry": "index.js",
    "outputPath": "./dist",
    "bundleName": "index.bundle",
    "noPlatformSuffix": true
  }
}
