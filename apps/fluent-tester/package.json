{
  "name": "@fluentui-react-native/tester",
  "version": "0.99.0",
  "description": "A test app to test FluentUI React Native Components during development",
  "license": "MIT",
  "author": "Microsoft <fluentuinativeowners@microsoft.com>",
  "homepage": "https://github.com/microsoft/fluentui-react-native",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "typings": "lib/index.d.ts",
  "onPublish": {
    "main": "lib-commonjs/index.js",
    "module": "lib/index.js"
  },
  "scripts": {
    "android": "react-native run-android",
    "build": "fluentui-scripts build",
    "bundle": "react-native rnx-bundle --dev false",
    "bundle:android": "react-native rnx-bundle --dev false --platform android --assets-path dist/res",
    "bundle:ios": "react-native rnx-bundle --dev false --platform ios",
    "bundle:macos": "react-native rnx-bundle --dev false --platform macos",
    "bundle:windows": "react-native rnx-bundle --dev false --platform windows",
    "clean": "fluentui-scripts clean",
    "depcheck": "fluentui-scripts depcheck",
    "e2etest:ios": "wdio run wdio.conf.ios.js",
    "e2etest:macos": "wdio run wdio.conf.macos.js",
    "e2etest:windows": "rimraf errorShots/* reports/* && wdio run wdio.conf.windows.js",
    "ios": "react-native run-ios",
    "just": "fluentui-scripts",
    "lint": "fluentui-scripts eslint",
    "macos": "react-native run-macos --scheme FluentTester",
    "prettier": "fluentui-scripts prettier",
    "prettier-fix": "fluentui-scripts prettier --fix true",
    "start": "react-native start",
    "windows": "react-native run-windows --arch x64 --sln windows/FluentTester.sln"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/fluentui-react-native.git",
    "directory": "apps/fluent-tester"
  },
  "dependencies": {
    "@fluentui-react-native/android-theme": ">=0.12.2 <1.0.0",
    "@fluentui-react-native/apple-theme": ">=0.15.0 <1.0.0",
    "@fluentui-react-native/avatar": "^1.2.11",
    "@fluentui-react-native/badge": ">=0.1.34 <1.0.0",
    "@fluentui-react-native/button": ">=0.25.1 <1.0.0",
    "@fluentui-react-native/default-theme": ">=0.15.2 <1.0.0",
    "@fluentui-react-native/dropdown": ">=0.4.2 <1.0.0",
    "@fluentui-react-native/experimental-activity-indicator": ">=0.6.4 <1.0.0",
    "@fluentui-react-native/experimental-avatar": ">=0.16.6 <1.0.0",
    "@fluentui-react-native/experimental-button": "0.16.28",
    "@fluentui-react-native/experimental-checkbox": "0.12.19",
    "@fluentui-react-native/experimental-expander": "0.4.5",
    "@fluentui-react-native/experimental-menu-button": ">=0.4.32 <1.0.0",
    "@fluentui-react-native/experimental-native-date-picker": ">=0.7.1 <1.0.0",
    "@fluentui-react-native/experimental-radio-group": "^0.5.0",
    "@fluentui-react-native/experimental-shadow": "0.2.21",
    "@fluentui-react-native/experimental-shimmer": "0.7.4",
    "@fluentui-react-native/experimental-tabs": "0.5.17",
    "@fluentui-react-native/experimental-text": ">=0.11.6 <1.0.0",
    "@fluentui-react-native/framework": ">=0.8.4 <1.0.0",
    "@fluentui-react-native/icon": "0.13.9",
    "@fluentui-react-native/interactive-hooks": ">=0.18.7 <1.0.0",
    "@fluentui-react-native/menu": "^1.0.38",
    "@fluentui-react-native/merge-props": ">=0.5.0 <1.0.0",
    "@fluentui-react-native/notification": "0.17.11",
    "@fluentui-react-native/stack": ">=0.7.8 <1.0.0",
    "@fluentui-react-native/switch": "^0.6.9",
    "@fluentui-react-native/text": ">=0.15.0 <1.0.0",
    "@fluentui-react-native/theme": ">=0.7.2 <1.0.0",
    "@fluentui-react-native/theme-types": ">=0.19.1 <1.0.0",
    "@fluentui-react-native/themed-stylesheet": "^1.4.0",
    "@fluentui-react-native/theming-utils": ">=0.15.2 <1.0.0",
    "@fluentui-react-native/win32-theme": ">=0.21.2 <1.0.0",
    "@fluentui/react-native": ">=0.32.24 <1.0.0",
    "@react-native-community/slider": "^4.2.0",
    "@react-native-menu/menu": "^0.1.2",
    "@react-native-picker/picker": "^2.2.1",
    "react": "17.0.2",
    "react-native": "^0.66.0",
    "react-native-macos": "^0.66.0",
    "react-native-svg": "^12.3.0",
    "react-native-windows": "^0.66.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/runtime": "^7.8.0",
    "@fluentui-react-native/eslint-config-rules": "^0.1.1",
    "@fluentui-react-native/scripts": "^0.1.1",
    "@rnx-kit/cli": "^0.9.57",
    "@rnx-kit/metro-config": "^1.2.26",
    "@types/jasmine": "3.5.10",
    "@types/react": "^17.0.2",
    "@types/react-native": "^0.66.0",
    "@wdio/appium-service": "7.23.0",
    "@wdio/cli": "7.23.0",
    "@wdio/jasmine-framework": "7.23.0",
    "@wdio/local-runner": "7.23.0",
    "@wdio/spec-reporter": "7.23.0",
    "appium": "2.0.0-beta.41",
    "appium-mac2-driver": "1.4.0",
    "appium-xcuitest-driver": "4.11.1",
    "appium-windows-driver": "2.0.7",
    "flow-bin": "^0.113.0",
    "metro-config": "^0.66.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "react": "17.0.2",
    "react-native": "^0.66.0",
    "react-native-macos": "^0.66.0",
    "react-native-svg": "^12.3.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-test-app": "^1.6.2",
    "react-native-windows": "^0.66.0",
    "react-test-renderer": "17.0.2",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.5.4",
    "webdriverio": "7.23.0"
  },
  "peerDependencies": {
    "@office-iss/react-native-win32": "^0.66.6",
    "react": "17.0.2",
    "react-native": "^0.66.0",
    "react-native-macos": "^0.66.0",
    "react-native-svg": "^12.3.0",
    "react-native-windows": "^0.66.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnx-kit": {
    "reactNativeVersion": "^0.66",
    "kitType": "app",
    "bundle": {
      "targets": [
        "android",
        "ios",
        "macos",
        "windows"
      ],
      "entryPath": "index.js",
      "detectCyclicDependencies": {
        "throwOnError": true
      },
      "detectDuplicateDependencies": {
        "throwOnError": false
      },
      "typescriptValidation": false
    },
    "capabilities": [
      "core-android",
      "core-ios",
      "core-macos",
      "core-windows",
      "react",
      "svg"
    ]
  },
  "depcheck": {
    "ignoreMatches": [
      "@fluentui-react-native/experimental-expander",
      "@fluentui-react-native/experimental-drawer"
    ]
  }
}
