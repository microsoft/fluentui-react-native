      "targets": [
        "android"
      ],
      "entryPath": "index.js"
    },
    "capabilities": [
      "core-android",
      "hermes",
      "react",
      "test-app"
    ]
  },
  "depcheck": {
    "ignoreMatches": [
      "@react-native-community/slider",
      "@react-native-picker/picker",
      "hermes-engine",
      "react-native-test-app"
    ]
