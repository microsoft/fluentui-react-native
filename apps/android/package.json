    "react-native": "^0.62.2",
    "react-native-test-app": "^0.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.1.0",
