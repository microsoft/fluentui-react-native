{
  "name": "@fluentui-react-native/tester-android",
  "version": "0.0.1",
  "private": true,
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "fluentui-scripts build",
    "just": "fluentui-scripts",
    "clean": "fluentui-scripts clean",
    "code-style": "fluentui-scripts code-style",
    "depcheck": "fluentui-scripts depcheck",
    "lint": "fluentui-scripts eslint",
    "start": "react-native start",
    "test": "fluentui-scripts jest",
    "bundle": "react-native rnx-bundle --dev false",
    "bundle-dev": "react-native rnx-bundle",
    "android": "react-native run-android ",
    "prettier": "fluentui-scripts prettier",
    "prettier-fix": "fluentui-scripts prettier --fix true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/fluentui-react-native.git",
    "directory": "apps/android"
  },
  "dependencies": {
    "@fluentui-react-native/experimental-drawer": "*",
    "@fluentui-react-native/tester": "^0.53.2",
    "@react-native-community/slider": "^4.1.12",
    "@react-native-picker/picker": "^1.16.7",
    "hermes-engine": "~0.7.0",
    "react": "17.0.1",
    "react-native": "^0.64.2",
    "react-native-svg": "^12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/runtime": "^7.8.0",
    "@fluentui-react-native/eslint-config-rules": "^0.1.1",
    "@rnx-kit/cli": "^0.9.57",
    "@rnx-kit/metro-config": "^1.2.26",
    "@fluentui-react-native/scripts": "^0.1.1",
    "flow-bin": "^0.113.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-test-app": "^1.0.12",
    "react-test-renderer": "17.0.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnx-kit": {
    "reactNativeVersion": "0.64",
    "kitType": "app",
    "bundle": {
      "targets": [
        "android"
      ],
      "entryPath": "index.js",
      "detectCyclicDependencies": {
        "throwOnError": true
      },
      "detectDuplicateDependencies": {
        "throwOnError": false
      },
      "typescriptValidation": false
    },
    "capabilities": [
      "core-android",
      "hermes",
      "react",
      "test-app"
    ]
  },
  "depcheck": {
    "ignoreMatches": [
      "@react-native-community/slider",
      "@react-native-picker/picker",
      "hermes-engine",
      "react-native-test-app",
      "@fluentui-react-native/experimental-drawer"
    ]
  }
}
