{
  "name": "@fluentui-react-native/tester-android",
  "version": "0.0.1",
  "private": true,
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "fluentui-scripts build",
    "just": "fluentui-scripts",
    "clean": "fluentui-scripts clean",
    "code-style": "fluentui-scripts code-style",
    "depcheck": "fluentui-scripts depcheck",
    "lint": "fluentui-scripts eslint",
    "start": "react-native start",
    "test": "fluentui-scripts jest",
    "bundle": "react-native rnx-bundle --dev false",
    "bundle-dev": "react-native rnx-bundle",
    "android": "react-native run-android ",
    "prettier": "fluentui-scripts prettier",
    "prettier-fix": "fluentui-scripts prettier --fix true"
  },
  "dependencies": {
    "@fluentui-react-native/tester": "^0.30.4",
    "@react-native-community/slider": "^3.0.3",
    "@react-native-picker/picker": "^1.9.11",
    "hermes-engine": "~0.5.0",
    "react": "16.13.1",
    "react-native": "^0.63.2",
    "react-native-svg": "^12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.1.0",
    "@rnx-kit/cli": "^0.5.27",
    "@uifabricshared/build-native": "^0.1.1",
    "@uifabricshared/eslint-config-rules": "^0.1.1",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-native-svg-transformer": "^0.14.3",
