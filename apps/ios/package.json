{
  "name": "FluentTester-ios",
  "version": "0.0.1",
  "private": true,
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "fluentui-scripts build",
    "just": "fluentui-scripts",
    "clean": "fluentui-scripts clean",
    "code-style": "fluentui-scripts code-style",
    "lint": "fluentui-scripts eslint",
    "start": "react-native start",
    "bundle": "fluentui-scripts metro --cli",
    "bundle-dev": "fluentui-scripts metro --dev --cli",
    "ios": "react-native run-ios --project-path src"
  },
  "dependencies": {
    "@fluentui-react-native/tester": "^0.0.1",
    "@uifabricshared/theming-react-native": "0.7.48",
    "@uifabricshared/themed-stylesheet": "0.3.49",
    "react": "16.11.0",
    "react-native": "0.62.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/cli-platform-ios": "3.0.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "@uifabricshared/build-native": "0.1.1",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "metroBundles": {
    "targets": [
      "ios"
    ],
    "entry": "index.js",
    "outputPath": "./dist",
    "bundleName": "index.bundle",
    "noPlatformSuffix": true
  }
}
