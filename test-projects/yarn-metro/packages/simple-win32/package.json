    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-svg": "^11.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@office-iss/rex-win32": "0.62.0-preview.3-tenantreactnativewin-12906",
    "@react-native-community/cli": "^4.9.0",
