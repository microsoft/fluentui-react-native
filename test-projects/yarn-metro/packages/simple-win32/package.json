  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "bundle": "npx react-native bundle --entry-file ./index.js --bundle-output ./index.bundle --platform win32",
    "bundle-dev": "npx react-native bundle --entry-file ./index.js --bundle-output ./index.bundle --dev --platform win32",
    "run-win32": "rex-win32 --bundle index --component BasicWin32Test --basePath . --pluginProps",
    "run-win32-web": "rex-win32 --bundle index --component BasicWin32Test --basePath . --useWebDebugger --useLiveReload --pluginProps",
    "run-win32-devmain": "rex-win32 --bundle index --component BasicWin32Test --basePath . --useDevMain --pluginProps"
  },
